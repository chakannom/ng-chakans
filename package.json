    "rxjs": "6.5.3",
    "sass": "1.23.7",
    "sass-loader": "8.0.0",
    "simple-progress-webpack-plugin": "1.1.2",
    "style-loader": "1.0.0",
    "thread-loader": "2.1.3",
    "to-string-loader": "1.1.6",
    "ts-loader": "6.2.1",
    "tsickle": "0.37.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "typescript": "3.5.3",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-merge": "4.2.2",
    "write-file-webpack-plugin": "4.5.1",
    "zone.js": "0.10.2"
  },
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "ngPackage": {
    "lib": {
      "entryFile": "src/main/public_api.ts"
    }
  },
  "lint-staged": {
    "{,src/**/}*.{md,json,ts,css,scss,yml}": [
      "prettier --write",
      "git add"
    ]
  },
  "scripts": {
    "cleanup": "rimraf dist/*",
    "format": "prettier --write \"{,src/**/}*.{md,json,js,ts,css,scss,yml}\"",
    "lint": "tslint --project tsconfig.json -e 'node_modules/**'",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm run webpack:dev",
    "test": "npm run lint && jest --config src/test/jest.conf.js",
    "test:watch": "npm run test -- --watch",
    "themes": "bundle-scss -m \"./src/main/**/*.scss\" -d \"./dist/scss/ng-chakans.scss\"",
    "build": "npm run cleanup && ng-packagr -p package.json && npm run themes",
    "release": "git push && git push --tags && npm publish dist/",
    "webpack:dev": "npm run webpack-dev-server -- --config webpack/webpack.dev.js --inline --hot --port=9060 --watch-content-base --env.stats=minimal",
    "webpack-dev-server": "node --max_old_space_size=4096 node_modules/webpack-dev-server/bin/webpack-dev-server.js"
  }
}
