    "core-js": "3.4.2",
    "css-loader": "3.2.0",
    "file-loader": "4.3.0",
    "fork-ts-checker-webpack-plugin": "3.1.0",
    "friendly-errors-webpack-plugin": "1.7.0",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-preset-angular": "8.0.0",
    "lint-staged": "9.4.3",
    "merge-jsons-webpack-plugin": "1.0.20",
    "ng-packagr": "5.7.1",
    "ngx-perfect-scrollbar": "8.0.0",
